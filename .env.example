# Copy to .env.local for local development.
NEXT_PUBLIC_APP_NAME=Commitment Match MVP
# Base64-encoded 32-byte key recommended.
APP_ENCRYPTION_KEY=change-this-to-a-strong-key

# Supabase (required for production persistence)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Optional: service role is for seed/admin scripts only.
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional: force Vercel Preview deployments into read-only mode.
# When true and VERCEL_ENV=preview, write endpoints return 503.
APP_PREVIEW_READ_ONLY=false

# Optional observability sink for error events (Sentry alternative).
# Example: your log pipeline or webhook collector endpoint.
OBSERVABILITY_WEBHOOK_URL=
